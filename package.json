{
  "name": "ghastly",
  "version": "0.1.4",
  "description": "A lightweight library that helps to modularize applications built with Discord.js.",
  "keywords": [
    "discord.js",
    "library",
    "plugin",
    "bot"
  ],
  "homepage": "https://github.com/hkwu/ghastly#readme",
  "bugs": {
    "url": "https://github.com/hkwu/ghastly/issues"
  },
  "license": "MIT",
  "author": "Kelvin Wu <kelvinwu1996@gmail.com>",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hkwu/ghastly.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:docs": "jsdoc2md src/**/*.js > docs/api.md",
    "watch": "babel --watch src --out-dir lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "mocha --require babel-register --recursive",
    "docs:dev": "docute docs --watch",
    "prepublishOnly": "npm run build && yarn test"
  },
  "dependencies": {
    "es6-error": "^4.0.0",
    "lodash": "^4.15.0",
    "string-argv": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "docute-cli": "^0.5.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.0.2"
  },
  "peerDependencies": {
    "discord.js": "^11.0.0"
  },
  "engines": {
    "node": ">=7"
  }
}
