{
  "name": "ghastly",
  "version": "0.1.4",
  "description": "A lightweight, pluggable library that helps to modularize applications built with Discord.js.",
  "keywords": [
    "discord.js",
    "library",
    "plugin",
    "bot"
  ],
  "homepage": "https://github.com/hkwu/ghastly#readme",
  "bugs": {
    "url": "https://github.com/hkwu/ghastly/issues"
  },
  "license": "MIT",
  "author": "Kelvin Wu <kelvinwu1996@gmail.com>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hkwu/ghastly.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "mocha --require babel-register --recursive",
    "prepublish": "npm run test && npm run build",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:publish": "npm run docs:build && cd _book && git init && git commit --allow-empty -m 'Update docs.' && git checkout -b gh-pages && git add . && git commit -am 'Update docs.' && git push git@github.com:hkwu/ghastly gh-pages --force"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "es6-error": "^4.0.0",
    "lodash": "^4.15.0",
    "string-argv": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "gitbook-cli": "^2.3.0",
    "mocha": "^3.0.2"
  },
  "peerDependencies": {
    "discord.js": "9.2 - 9"
  }
}
